<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <!-- build:css /dist/css/app.css -->
    <link rel="stylesheet" href="/app/styles/css/app.css" />
    <!-- /build -->
    <meta name="viewport" content="width=device-width, user-scalable=no">
    <title>SteamDating</title>
  </head>
  <body>
    <div id="app">
      Loading...
    </div>
    <script>
      {{{state}}}
    </script>
    <!-- build:js /dist/js/app.js -->
    <script src="/lib/system.js"></script>
    <script src="/config.js"></script>
    <script>
      window.STEAMDATING_CONFIG.debug = true;
      System.trace = true;
      System.import('app/app.js');
    </script>
    <!-- /build -->
    <script>
      function init() {
      if(window.STEAMDATING_APP) window.STEAMDATING_APP.init();
      else window.setTimeout(init, 0);
      }
      init();
    </script>
  </body>
</html>
